;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns1-digicore23.test usuari.digicore23.test. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;name server NS records
	INS	NS	ns1.digicore-23.test.

; registers
ns1	IN	A	192.168.4.19
server	IN	A	192.168.4.19
dbserverIN	A	192.168.4.119
data	IN	CNAME	dbserver
